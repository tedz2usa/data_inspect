#!/usr/bin/env ruby

require 'data_inspect'
require 'awesome_print'

puts DataInspect.version

puts DataInspect.hi



def main

  options = ARGV

  filename = options.last

  puts filename

  
  DataInspect.readfile(filename) do |reader|

    while !reader.atEOS?
      puts reader.next_byte
    end


    

  end




end




main



